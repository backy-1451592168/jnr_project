<!--更新日志-->
<view class="updateLog">
  <navbar isBackspace="{{true}}" title="更新日志"></navbar>
  <view class="box">
    <view class="options" wx:for="{{updateData}}" wx:key="index">
      <view class="title">{{item.title}}</view>
      <view class="content">
        <view class="li" wx:for="{{item.data}}" wx:key="i">{{item}}</view>
      </view>
    </view>
  </view>
</view>